[run]
branch = True
source = tools/supplychain

[report]
# Exclude import guard lines that are not testable without uninstalling dependencies
exclude_lines =
    # Import guard blocks
    except ImportError:
    print.*Install with:
    sys.exit\(1\)
    
    # Standard pragma
    pragma: no cover
    
    # Don't complain about missing debug-only code
    def __repr__
    if self\.debug
    
    # Don't complain if tests don't hit defensive assertion code
    raise AssertionError
    raise NotImplementedError
    
    # Don't complain if non-runnable code isn't run
    if __name__ == .__main__.:
    if TYPE_CHECKING:

omit =
    */tests/*
    */__pycache__/*
    */site-packages/*
